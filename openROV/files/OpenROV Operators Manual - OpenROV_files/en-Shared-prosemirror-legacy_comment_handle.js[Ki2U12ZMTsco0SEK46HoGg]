var editorMapping=window.editorMapping||{};function LegacyCommentHandle(a){this.elem=a}LegacyCommentHandle.prototype={get value(){return this.elem.value},get text(){return this.elem.value},get el(){return this.elem},get editable(){return this.elem},clear:function(){this.elem.value=""},focus:function(){this.editable.activate()},reset:function(){},saveState:function(){},disableEnter:function(){},enableEnter:function(){}};
function setupLegacyEditHandle(a,b){var c=b.parentNode.parentNode.id,d=new LegacyCommentHandle(b);a[c]=d;b.dataset.editorId=c;return d}Array.prototype.forEach.call(document.getElementsByClassName("commentEdit"),setupLegacyEditHandle.bind(null,editorMapping));window.setupLegacyEditHandle=setupLegacyEditHandle;window.editorMapping=editorMapping;
